{
  "name": "@orctane/node",
  "version": "1.0.0",
  "description": "Node.js library for the Orctane API",
  "main": "index.js",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "test": "jest",
    "test:watch": "jest --watch",
    "format:apply": "biome check --apply .",
    "format:check": "biome format .",
    "format": "biome format --write .",
    "lint": "biome check .",
    "prepublishOnly": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orctane/orctane-node.git"
  },
  "author": "Orctane",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/orctane/orctane-node/issues"
  },
  "homepage": "https://github.com/orctane/orctane-node#readme",
  "devDependencies": {
    "@biomejs/biome": "^1.8.1",
    "@types/node": "^20.14.2",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "ts-jest": "^29.1.4",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "axios": "^1.7.2"
  }
}
