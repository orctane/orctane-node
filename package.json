{
  "name": "@orctane/node",
  "version": "1.2.1",
  "description": "Node.js library for the Orctane API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "test": "jest",
    "example": "bun ./example/index.ts",
    "test:watch": "jest --watch",
    "format:apply": "biome check --apply .",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts",
    "prepublishOnly": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orctane/orctane-node.git"
  },
  "author": "Orctane",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/orctane/orctane-node/issues"
  },
  "homepage": "https://github.com/orctane/orctane-node#readme",
  "devDependencies": {
    "@biomejs/biome": "^1.8.1",
    "@types/node": "^20.14.2",
    "@types/nodemailer": "^6.4.15",
    "eslint": "^9.9.0",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^3.3.3",
    "ts-jest": "^29.1.4",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@aws-sdk/client-sesv2": "^3.621.0",
    "axios": "^1.7.2",
    "nodemailer": "^6.9.14"
  },
  "packageManager": "pnpm@9.7.0"
}
